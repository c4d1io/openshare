# =========================
# Django / Python Gitignore
# =========================

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# Virtual environment
env/
venv/
.venv/
ENV/

# Django stuff
*.log
local_settings.py
db.sqlite3
db.sqlite3-journal

# Media & static (optional: keep if needed)
media/
staticfiles/

# Secret environment variables
.env

# Migration files (optional: usually keep them)
# **/migrations/*.py
# **/migrations/*.pyc

# Pytest / coverage
.pytest_cache/
.coverage
htmlcov/

# VS Code
.vscode/

# PyCharm
.idea/

# MacOS
.DS_Store

# Windows
Thumbs.db

# Jupyter Notebook
.ipynb_checkpoints/

# Build / packaging
build/
dist/
*.egg-info/

# Node modules (if frontend included)
node_modules/

# Celery beat schedule
celerybeat-schedule

# Redis dump
dump.rdb
